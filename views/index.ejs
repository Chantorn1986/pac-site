      <script>
        function runDocNumber() {
          let departmentID = document.forms['formT']['departmentID'].value;
          if (departmentID === "Sales & Marketing Dept.") {
            document.forms['formT']['docNo'].value = (new Date().toLocaleDateString('th-TH', { year: '2-digit' })).substr(5) + "02"
          } else if (departmentID === "Product & Technical Support Dept.") {
            document.forms['formT']['docNo'].value = (new Date().toLocaleDateString('th-TH', { year: '2-digit' })).substr(5) + "04"
          } else if (departmentID === "Procurement Dept.") {
            document.forms['formT']['docNo'].value = (new Date().toLocaleDateString('th-TH', { year: '2-digit' })).substr(5) + "05"
          } else if (departmentID === "Financial Dept.") {
            document.forms['formT']['docNo'].value = (new Date().toLocaleDateString('th-TH', { year: '2-digit' })).substr(5) + "06"
          } else if (departmentID === "Information Technology Dept.") {
            document.forms['formT']['docNo'].value = (new Date().toLocaleDateString('th-TH', { year: '2-digit' })).substr(5) + "08"
          } else if (departmentID === "Human Resource Dept.") {
            document.forms['formT']['docNo'].value = (new Date().toLocaleDateString('th-TH', { year: '2-digit' })).substr(5) + "08"
          } else {
            document.forms['formT']['docNo'].value = (new Date().toLocaleDateString('th-TH', { year: '2-digit' })).substr(5)
          }
        }
        function showMessage() {
          alert("This is a internal script!");
        }
      </script>

