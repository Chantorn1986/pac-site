<!DOCTYPE html>
<html lang="en">

<head>

  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="">

  <title>PAC APP - <%= title %>
  </title>

  <!-- Custom fonts for this template -->
  <link href="/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
  <link
    href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
    rel="stylesheet">

  <!-- Custom styles for this template -->
  <link href="/css/sb-admin-2.min.css" rel="stylesheet">

  <!-- Custom styles for this page -->
  <link href="/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">

</head>

<body id="page-top">

  <!-- Page Wrapper -->
  <div id="wrapper">

    <%- include('../partials/sidebar'); %>

      <!-- Content Wrapper -->
      <div id="content-wrapper" class="d-flex flex-column">

        <!-- Main Content -->
        <div id="content">

          <!-- Topbar -->
          <%- include('../partials/topbar'); %>
            <!-- End of Topbar -->

            <!-- Begin Page Content -->
            <div class="container-fluid">
              <div class="card o-hidden border-0 shadow-lg ">
                <div class="card-header py-3">
                  <h6 class="m-0 font-weight-bold text-primary"><i class="fas fa-user fa-fw"></i> Employee Management -
                    <%= title %>
                  </h6>
                </div>

                <div class="card-body p-0">
                  <!-- Nested Row within Card Body -->
                  <div class="row">
                    <!--<div class="col-lg-2 d-none d-lg-block"></div>-->
                    <div class="col-lg-12">
                      <div class="p-4">

                        <h6 class="m-0 font-weight-bold text-primary mb-3"><i class="fas fa-car-side fa-fw"></i> / <i
                            class="fas fa-motorcycle fa-fw"></i>
                          <%= title %>
                        </h6>
                        <div class="form-group row">
                          <div class="col-sm-6">
                            <label for="vehicleE" class="form-label">Conveyance</label>
                            <input type="text" name="vehicleE" class="form-control" value="<%= emCar.vehicle %>"
                              readonly>


                          </div>
                          <div class="col-sm-6">
                            <label for="carBrandE" class="form-label">Brand</label>
                            <input type="text" name="carBrandE" class="form-control" value="<%= emCar.brand %>"
                              readonly>
                          </div>

                        </div>
                        <div class="form-group row">
                          <div class="col-sm-4">
                            <label for="carModelE" class="form-label">Model</label>
                            <input type="text" name="carModelE" class="form-control" value="<%= emCar.model %>"
                              readonly>
                          </div>
                          <div class="col-sm-4">
                            <label for="carColorE" class="form-label">Color</label>
                            <input type="text" name="carColorE" class="form-control" value="<%= emCar.color %>"
                              readonly>
                          </div>
                          <div class="col-sm-4">
                            <label for="carRegistrationE" class="form-label">Registration</label>
                            <input type="text" name="carRegistrationE" class="form-control"
                              value="<%= emCar.registration %>" readonly>
                          </div>
                        </div>
                        <div class="form-group row">
                          <div class="col-sm-6">
                            <label for="createdAtCarE" class="form-label">CreatedAt</label>
                            <% if (coverDate.createdAt) { %>
                              <input type="text" name="createdAtCarE" class="form-control"
                                value="<%= coverDate.createdAt %>" readonly>
                              <% } else { %>
                                <input type="text" class="form-control" name="createdAtCarE" value="-" readonly>
                                <% } %>

                          </div>
                          <div class="col-sm-6">
                            <label for="updatedAtE" class="form-label">UpdatedAt</label>
                            <% if (coverDate.updatedAt) { %>
                              <input type="text" name="updatedAtCarE" class="form-control"
                                value="<%= coverDate.updatedAt %>" readonly>
                              <% } else { %>
                                <input type="text" class="form-control" name="updatedAtCarE" value="-" readonly>
                                <% } %>
                          </div>
                        </div>
                        <hr>
                        <div class="text-center">
                          <a class="small" href="/employee/Sub/<%= emCar.emID %>">Back to ...</a>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <!--<div class="col-lg-2 d-none d-lg-block"></div>-->
            </div>
            <!-- /.container-fluid -->

        </div>
        <!-- End of Main Content -->

        <!-- Footer -->
        <%- include('../partials/footer'); %>
          <!-- End of Footer -->

      </div>
      <!-- End of Content Wrapper -->

  </div>
  <!-- End of Page Wrapper -->

  <!-- Scroll to Top Button-->
  <a class="scroll-to-top rounded" href="#">
    <i class="fas fa-angle-up"></i>
  </a>

  <!-- Bootstrap core JavaScript-->
  <script src="/vendor/jquery/jquery.min.js"></script>
  <script src="/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

  <!-- Core plugin JavaScript-->
  <script src="/vendor/jquery-easing/jquery.easing.min.js"></script>

  <!-- Custom scripts for all pages-->
  <script src="/js/sb-admin-2.min.js"></script>

  <!-- Page level plugins -->
  <script src="/vendor/datatables/jquery.dataTables.min.js"></script>
  <script src="/vendor/datatables/dataTables.bootstrap4.min.js"></script>

  <!-- Page level custom scripts -->
  <script src="/js/demo/datatables-demo.js"></script>

</body>

</html>