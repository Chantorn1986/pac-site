<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="">

  <title>PAC APP - <%= title %>
  </title>

  <!-- Custom fonts for this template -->
  <link href="/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
  <link
    href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
    rel="stylesheet">

  <!-- Custom styles for this template -->
  <link href="/css/sb-admin-2.min.css" rel="stylesheet">

  <!-- Custom styles for this page -->
  <link href="/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">

</head>

<body id="page-top">

  <!-- Page Wrapper -->
  <div id="wrapper">

    <%- include('../partials/sidebar'); %>

      <!-- Content Wrapper -->
      <div id="content-wrapper" class="d-flex flex-column">

        <!-- Main Content -->
        <div id="content">

          <!-- Topbar -->
          <%- include('../partials/topbar'); %>
            <!-- End of Topbar -->

            <!-- Begin Page Content -->
            <div class="container-fluid">

              <!-- Collapsable Card Profile -->
              <div class="card shadow mb-4">
                <!-- Card Header - Accordion -->
                <a href="#collapseGoods" class="d-block card-header py-3" data-toggle="collapse" role="button"
                  aria-expanded="true" aria-controls="collapseCardProfile">
                  <h6 class="m-0 font-weight-bold text-primary">
                    <i class="fas fa-fw fa-archive"></i> Goods Management - <%= stockCardGoods.code %> : <%=
                        stockCardGoods.model %>
                  </h6>
                </a>
                <!-- Card Content - Collapse -->
                <div class="collapse show" id="collapseGoods">
                  <div class="card-body">
                    <div class="row mb-2">
                      <div class="col-2 text-right"><strong>No. : </strong></div>
                      <div class="col-4">
                        <%= stockCardGoods.no %>
                      </div>
                      <div class="col-2 text-right"><strong>Length (m.) : </strong></div>
                      <div class="col-4">
                        <%= stockCardGoods.length %>
                      </div>
                    </div>
                    <div class="row mb-2">
                      <div class="col-2 text-right"><strong>Code : </strong></div>
                      <div class="col-10">
                        <%= stockCardGoods.code %>
                      </div>
                    </div>
                    <div class="row mb-2">
                      <div class="col-2 text-right"><strong>Model : </strong></div>
                      <div class="col-10">
                        <%= stockCardGoods.model %>
                      </div>
                    </div>
                    <div class="row mb-2">
                      <div class="col-2 text-right"><strong>Brand : </strong></div>
                      <div class="col-10">
                        <%= stockCardGoods.brand %>
                      </div>
                    </div>
                    <div class="row mb-2">
                      <div class="col-2 text-right"><strong>Full Keyword : </strong></div>
                      <div class="col-10">
                        <%= stockCardGoods.keyword %>
                      </div>
                    </div>
                    <div class="row mb-2">
                      <div class="col-2 text-right"><strong>Price : </strong></div>
                      <div class="col-4">
                        <%= stockCardGoods.price %>
                      </div>
                      <div class="col-2 text-right"><strong>Limit Price : </strong></div>
                      <div class="col-4">
                        <%= stockCardGoods.limitPrice %>
                      </div>
                    </div>
                    <div class="row mb-2">
                      <div class="col-2 text-right"><strong>Shelf : </strong></div>
                      <div class="col-4">
                        <%= stockCardGoods.shelf %>
                      </div>
                      <div class="col-2 text-right"><strong>Remain : </strong></div>
                      <div class="col-4">
                        <%= stockCardGoods.remain %>
                      </div>
                    </div>
                    <div class="row mb-2">
                      <div class="col-2 text-right"><strong>Descrition for Sale : </strong></div>
                      <div class="col-10">
                        <%= stockCardGoods.remarkPurchase %>
                      </div>
                    </div>
                    <div class="row mb-2">
                      <div class="col-2 text-right"><strong>Remark : </strong></div>
                      <div class="col-10">
                        <%= stockCardGoods.remarkSale %>
                      </div>
                    </div>
                    <div class="row">
                      <div class="col-2 text-right"><strong>Created : </strong></div>
                      <div class="col-4">
                        <%= stockCardGoods.createdAt %>
                      </div>
                      <div class="col-2 text-right"><strong>Updated : </strong></div>
                      <div class="col-4">
                        <%= stockCardGoods.updatedAt %>
                      </div>
                    </div>
                    <br>
                    <div class="row p-3">
                      <div class="col-2">
                        <div class="card border-left-success shadow">
                          <div class="card-body">
                            <div class="row no-gutters align-items-center">
                              <div class="col mr-2">
                                <div class="text-xs font-weight-bold text-success text-uppercase mb-1">
                                  คงเหลือ</div>
                                <div class="h4 mb-0 font-weight-bold text-gray-800">
                                  <%= resultsSum.balance %>
                                </div>
                              </div>
                              <div class="col-auto">
                                <i class="fas fa-fw fa-archive fa-2x text-gray-300"></i>
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>
                      <div class="col-8">
                        <table class="table table-bordered">
                          <tr>
                            <th class="text-center bg-success text-white">รับ</th>
                            <% if(resultsSum.SecFull>0){ %>
                              <th class="text-center bg-success text-white">ท่อนเต็ม</th>
                              <% } %>
                                <th class="text-center bg-success text-white">ยืม</th>
                                <th class="text-center bg-success text-white">จอง</th>
                                <th class="text-center bg-success text-white">คืนจอง</th>
                                <th class="text-center bg-warning text-gray-900">ขาย</th>
                                <th class="text-center bg-gray-400 text-gray-900">คืน</th>
                                <th class="text-center bg-primary text-white">กำลังเข้า</th>
                                <th class="text-center bg-gray-200 text-gray-900">PO</th>
                          </tr>
                          <tr>
                            <td class="text-center">
                              <%= resultsSum.receiveQty %>
                            </td>
                            <% if(resultsSum.SecFull>0){ %>
                              <td class="text-center">
                                <%= resultsSum.SecFull %>
                              </td>
                              <% } %>
                                <td class="text-center">
                                  <%= resultsSum.loaningQty %>
                                </td>
                                <td class="text-center">
                                  <%= resultsSum.bookingQty %>
                                </td>
                                <td class="text-center">
                                  <%= resultsSum.retrunBookingQty %>
                                </td>
                                <td class="text-center">
                                  <%= resultsSum.saleQty %>
                                </td>
                                <td class="text-center">
                                  <%= resultsSum.retrunQty %>
                                </td>
                                <td class="text-center">
                                  <%= resultsSum.comingSoonQty %>
                                </td>
                                <td class="text-center">
                                  <%= resultsSum.poQty %>
                                </td>
                          </tr>
                        </table>
                      </div>
                      <div class="col-2">
                        <div class="row d-flex">
                          <a href="/stockCard/good/Edit/<%= stockCardGoods.id %>"
                            class="btn btn-warning btn-icon-split ml-auto btn-sm">
                            <span class="icon text-white-50">
                              <i class="fas fa-pen"></i>
                            </span>
                            <span class="text">Edit Goods</span>
                          </a>
                        </div>
                      </div>
                    </div>

                  </div>
                </div>
              </div>
            </div>
        </div>
        <div id="content">
          <div class="container-fluid">

            <!-- DataTales Example -->
            <div class="card shadow mb-4">
              <div class="card-header py-3 d-flex">
                <h6 class="m-0 font-weight-bold text-primary"><i class="fas fa-money-check"></i>
                  <%= title %>
                </h6>
                <!--<a href="/tablesAdd" class="btn btn-info btn-circle btn-sm ml-auto"><i class="fas fa-plus"></i></a>-->
                <a href="/stockCard/Add/<%= stockCardGoods.id %>" class="btn btn-primary btn-icon-split ml-auto btn-sm">
                  <span class="icon text-white-50">
                    <i class="fas fa-plus-circle"></i>
                  </span>
                  <span class="text">Create Stock Card</span>
                </a>

              </div>
              <div class="card-body">
                <div class="table-responsive">
                  <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                      <tr>
                        <th>Date</th>
                        <th>Code</th>
                        <th>Model</th>
                        <th>Document</th>
                        <th>Status</th>
                        <th>Qty</th>
                        <% if(resultsSum.SecFull>0){ %>
                          <th>Full</th>
                          <% } %>
                            <th>Note</th>
                            <th class="text-center">Action</th>
                      </tr>
                    </thead>
                    <tfoot>
                      <tr>
                        <th>Date</th>
                        <th>Code</th>
                        <th>Model</th>
                        <th>Document</th>
                        <th>Status</th>
                        <th>Qty</th>
                        <% if(resultsSum.SecFull>0){ %>
                          <th>Full</th>
                          <% } %>
                            <th>Note</th>
                            <th class="text-center">Action</th>
                      </tr>
                    </tfoot>
                    <tbody>
                      <% stockCard.forEach(stockCard=> { %>
                        <tr>
                          <td>
                            <%= stockCard.cardDate %>
                          </td>
                          <td>
                            <%= stockCard.goodsCode %>
                          </td>
                          <td>
                            <%= stockCard.model %>
                          </td>
                          <td>
                            <%= stockCard.document %>
                          </td>
                          <td>
                            <%= stockCard.statusName %>
                          </td>
                          <td class="text-right">
                            <%= stockCard.qty %>
                          </td>
                          <% if(resultsSum.SecFull>0){ %>
                            <td class="text-right">
                              <%= stockCard.sectionFull %>
                            </td>
                            <% } %>
                              <td>
                                <%= stockCard.note %>
                              </td>
                              <td class="align-middle text-center">
                                <a href="/stockCard/View/<%= stockCard.id %>" class="btn btn-info btn-circle btn-sm">
                                  <i class="fas fa-eye"></i></a>
                                <a href="/stockCard/Edit/<%= stockCard.id %>" class="btn btn-warning btn-circle btn-sm">
                                  <i class="fas fa-pen"></i></a>
                                <a href="/stockCard/Del/<%= stockCard.id %>" class="btn btn-danger btn-circle btn-sm"
                                  onclick="return confirm('Are you sure ?');">
                                  <i class="fas fa-trash-alt"></i></a>

                              </td>
                        </tr>
                        <% }) %>
                    </tbody>
                  </table>
                </div>
                <hr>
                <div class="text-center">
                  <a class="small" href="/stockCard/ReportBackEnd">Back to ...</a>
                </div>
              </div>

            </div>

          </div>


        </div>
        <!-- End of Main Content -->

        <!-- Footer -->
        <%- include('../partials/footer'); %>
          <!-- End of Footer -->

      </div>
      <!-- End of Content Wrapper -->

  </div>
  <!-- End of Page Wrapper -->

  <!-- Scroll to Top Button-->
  <a class="scroll-to-top rounded" href="#">
    <i class="fas fa-angle-up"></i>
  </a>

  <!-- Bootstrap core JavaScript-->
  <script src="/vendor/jquery/jquery.min.js"></script>
  <script src="/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

  <!-- Core plugin JavaScript-->
  <script src="/vendor/jquery-easing/jquery.easing.min.js"></script>

  <!-- Custom scripts for all pages-->
  <script src="/js/sb-admin-2.min.js"></script>

  <!-- Page level plugins -->
  <script src="/vendor/datatables/jquery.dataTables.min.js"></script>
  <script src="/vendor/datatables/dataTables.bootstrap4.min.js"></script>

  <!-- Page level custom scripts -->
  <script src="/js/demo/datatables-demo.js"></script>

</body>

</html>